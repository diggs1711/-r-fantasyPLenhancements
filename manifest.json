{
    "name": "FPL Stats Insights",
    "version": "0.0.1.3",
    "description": "Provides quick and easy way to view live FPL Data from your browser",
    "manifest_version": 2,
    "permissions": ["activeTab", "declarativeContent", "storage", "https://fantasy.premierleague.com/drf/bootstrap-static"],
      "browser_action": {
        "default_popup": "index.html",
        "default_icon": {
          "128" : "imgs/logo.png"
          }
      },
      "icons": {
        "128" : "imgs/logo.png"
      }
  }