{
    "name": "FPL Enchancement",
    "version": "1.0",
    "description": "FPL enhancement tool",
    "manifest_version": 2,
    "options_page": "options.html",
    "permissions": ["activeTab", "declarativeContent", "storage", "https://fantasy.premierleague.com/drf/bootstrap-static"],
    "background": {
        "scripts": ["enhancement.js"],
        "persistent": false
      },
      "browser_action": {
        "default_popup": "index.html",
        "default_icon": {
            "16": "images/get_started16.png",
            "32": "images/get_started32.png",
            "48": "images/get_started48.png",
            "128": "images/get_started128.png"
          }
      },
      "icons": {
        "16": "images/get_started16.png",
        "32": "images/get_started32.png",
        "48": "images/get_started48.png",
        "128": "images/get_started128.png"
      }
  }